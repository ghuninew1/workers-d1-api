{
  "name": "@ghuninew1/workers-d1-api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "deploy": "export NO_D1_WARNING=true && wrangler deploy --minify",
    "dev": "export NO_D1_WARNING=true && wrangler dev --port 3000 -r ",
    "kv": "export NO_D1_WARNING=true && wrangler kv:key put --binding=basic --key=foo --value=bar",
    "dbcre": "export NO_D1_WARNING=true && wrangler d1 execute basic --file='./db.sql'",
    "viewdb": "export NO_D1_WARNING=true && wrangler d1 execute basic --command='SELECT * FROM data'"
  },
  "devDependencies": {
    "@cloudflare/kv-asset-handler": "^0.3.0",
    "@miniflare/cache": "^2.14.1",
    "@miniflare/core": "^2.14.1",
    "@miniflare/storage-memory": "^2.14.1",
    "hono": "^3.5.6",
    "wrangler": "^3.6.0",
    "ws": "^8.13.0"
  }
}
